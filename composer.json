{
    "name": "chadchapman/chappy-php-starter",
    "description": "Starter project for Chappy PHP Framework.",
    "type": "project",
    "license": "MIT",
    "authors": [
      {
        "name": "Chad Chapman",
        "email": "chad.chapman2010@gmail.com"
      }
    ],
    "require": {
      "php": ">=8.3",
        "chadchapman/chappy-php-framework": "@dev"
    },
    "repositories": [
      {
        "type": "path",
        "url": "../chappy-php-framework",
        "options": {
          "symlink": true
        }
      }
    ],
    "autoload": {
      "psr-4": {
        "App\\": "app/",
        "Config\\": "config/",
        "Database\\Seeders\\": "database/seeders/",
        "Database\\Migrations\\": "database/migrations/"
      }
    },
    "scripts": {
      "post-create-project-cmd": [
        "@php vendor/chadchapman/chappy-php-framework/src/scripts/setup.php"
      ]
    }
  }
